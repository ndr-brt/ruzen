{
	"name": "ruzen",
	"displayName": "ruzen",
	"description": "vscode plugin for ruzen",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ruzen.eval"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ruzen.eval",
				"title": "Ruzen: eval line"
			}
		],
        "keybindings": [
            {
                "command": "ruzen.eval",
                "key": "Shift+Enter"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
